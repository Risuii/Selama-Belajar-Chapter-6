<html>
  <head>
    <Title>
      Article | All
    </Title>
  </head>

  <body>
    <% articles.forEach((i) => { %>
      <div>
        <h1>
          <%= i.title %>
        </h1>

        <p>
          <%= i.body %>
        </p>

        <a href="/articles/<%= i.id %>">
          Baca lebih lanjut
        </a>
        
        <br>
        
        <a href="/articles/update/<%= i.id %>">
          Update
        </a>
        
        <br>
        
        <a href="/articles/delete/<%= i.id %>">
          Hapus
        </a>
      </div>
    <% }) %>
  </body>
</html>